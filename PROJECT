- api:
    crdVersion: v1
    namespaced: true
  controller: true
  domain: gitctl.com
  group: apps
  kind: Application
  path: github.com/gitctl-pro/apps/apis/v1
  version: v1
- api:
    crdVersion: v1
    namespaced: true
  controller: true
  domain: gitctl.com
  group: apps
  kind: Canary
  path: github.com/gitctl-pro/apps/apis/v1
  version: v1
- api:
    crdVersion: v1
    namespaced: true
  controller: true
  domain: gitctl.com
  group: core
  kind: Cluster
  path: github.com/gitctl-pro/apps/apis/core/v1
  version: v1
version: "3"
